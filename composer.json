{
    "name": "craftpulse/craft-year-picker",
    "description": "This plugin provides the user with a date picker only for years",
    "type": "craft-plugin",
    "license": "mit",
    "support": {
        "email": "craftpulse.plugins@gmail.com",
        "issues": "https://github.com/craftpulse/craft-year-picker/issues?state=open",
        "source": "https://github.com/craftpulse/craft-year-picker",
        "docs": "https://github.com/craftpulse/craft-year-picker",
        "rss": "https://github.com/craftpulse/craft-year-picker/releases.atom"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.5.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "craftpulse\\yearpicker\\": "src/"
        }
    },
    "extra": {
        "handle": "year-picker",
        "name": "Year Picker",
        "developer": "Craftpulse",
        "documentationUrl": "https://github.com/craftpulse/craft-year-picker",
        "class": "craftpulse\\yearpicker\\YearPicker"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

